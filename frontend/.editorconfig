[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4

[{.babelrc, .stylelintrc, jest.config, eslintrc, *.bowerrc, *.jsb3, *.jsb2, *.json}]
indent_style = space
indent_size = 2

[*.feature]
indent_style = space
indent_size = 2

[{*.applejs, *.js}]
indent_style = space
indent_size = 2

[*.js.flow]
indent_style = space
indent_size = 2

[*.js.map]
indent_style = space
indent_size = 2

[{*.yml, *.yaml}]
indent_style = space
indent_size = 2
